package SortIncome

import LinkedList

function sortIncome()
    let incomeIds = udg_Temp_UG.asList()
    incomeIds.sortWith((u1, u2) -> u2.getPointValue() - u1.getPointValue())
    udg_Temp_UG.clear()
    for u from incomeIds.staticItr()
        udg_Temp_UG.addUnit(u)


init
    BeforeSendIncome.addCondition(Condition(function sortIncome))
